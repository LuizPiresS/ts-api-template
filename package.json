{
  "name": "ts-api-template",
  "version": "1.0.0",
  "description": "Site pessoal do dev que vos fala\"",
  "main": "index.js",
  "repository": "git@github.com:LuizPiresS/ts-api-template.git",
  "author": "Luiz Pires <kw13code@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && node dist/index.js",
    "start:dev": "tsnd --respawn --exit-child --debounce --clear --transpile-only --inspect --no-notify src/index.ts",
    "build": "tsc",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test:unit": "jest --passWithNoTests -c ./jest-unit-config.js",
    "test:functional": "jest  --passWithNoTests --runInBand -c './jest-integration-config.js'"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.13",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.10.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "git-commit-msg-linter": "^2.4.2",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.6",
    "prettier": "^2.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  }
}
